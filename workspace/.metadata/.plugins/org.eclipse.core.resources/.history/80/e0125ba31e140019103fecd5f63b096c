package metier;

public class CompteEnBanque extends Thread{
	private double solde;

	public synchronized void  crediter(double montant) {
		solde = solde + montant;
	} // Ô¨Ån de crediter

	public double consulter() {
		return solde;
	}
	
	public void run()
	{
		Thread t1 = new Thread();
		Thread t2 = new Thread();
	}
}