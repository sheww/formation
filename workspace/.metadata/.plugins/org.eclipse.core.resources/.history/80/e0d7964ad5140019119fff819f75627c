package metier;

import java.util.ArrayList;

public class ArrayThread {
	private ArrayList<MyThread> l;
	
	public ArrayThread(int number, CompteEnBanque c)
	{
		for(int i=0; i < number; i++)
		{
			l.add(new MyThread("t"+i,c));
		}
	}
	
	public void go()
	{
		for(int i=0; i < l.size(); i++)
		{
			
		}
	}
}
